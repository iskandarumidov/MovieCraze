    <link href="@Url.Content("~/Content/New_Design/style_full_screen_search.css")" rel="stylesheet">
    <link rel="stylesheet" href='@Url.Content("~/Content/New_Design/style.css")'>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src='http://cdnjs.cloudflare.com/ajax/libs/jquery.actual/1.0.17/jquery.actual.min.js'></script>
